{
  "expo": {
    "name": "Mockgram-RN",
    "description": "This project is really great.",
    "slug": "Mockgram-RN",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.molinz.mockgram"
    },
    "android": {
      "package": "com.molinz.mockgram"
    },
    "scheme": "mockgram"
  }
}